<!doctype html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">

  <!-- Bootstrap CSS -->
  
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
  <!-- <link rel="stylesheet" type="text/css" href="/fonts/font-awesome-4.7.0/css/font-awesome.min.css"> -->
  <link rel="stylesheet" type="text/css" href="/stylesheets/bootstrap.min.css">

  <link rel="stylesheet" href="/stylesheets/util.css">
    <link rel="stylesheet" href="/stylesheets/main.css">

  <title>mbti 게시판</title>
</head>

<body class="text-center" style="background-image: url(/images/bg.jpg); margin-top: 50px; margin-left: 200px; margin-right: 200px; ">
  <a><img style="align-content: center;" src="/images/MBTIBoard.png" width="450" height="90"></a>
  <br><br><br>
  <table class="table table-striped table-hover" >
    <thead>
      <tr>
        <th scope="col">게시글 번호</th>
        <th scope="col">게시글 제목</th>
        <th scope="col">게시글 작성 일시</th>
        <th scope="col">게시판 번호</th>
        <th scope="col">회원 아이디</th>
        <th scope="col"></th>
      </tr>
    </thead>
    <tbody>
      <%
      for(var i=0; i<mbtiBoard.length; i++) {
      %>
      <tr>
        <th><%= mbtiBoard[i].post_id %></th>
        <th><a href='/mbti/detail/<%= mbtiBoard[i].post_id %>'><%= mbtiBoard[i].post_title %></a></th>
        <th><%= mbtiBoard[i].post_writingTime %></th>
        <th><%= mbtiBoard[i].board_board_id %></th>
        <th><%= mbtiBoard[i].user_user_id %></th>
      </tr>
      <% } %>
    </tbody>
  </table>
  
  <div class="d-grid gap-2 d-md-flex justify-content-md-end">
    <button class="btn me-md-2 btn-dark" type="button" onclick='location.href="/mbti/board/register"'>글 등록</button>
  </div>

  <nav aria-label="Page navigation example">
    <ul class="pagination justify-content-center">
      <li class="page-item disabled">
        <a class="page-link">Previous</a>
      </li>
      <li><a class="page-link" href="#">1</a></li>
      <li class="page-item"><a class="page-link" href="#">2</a></li>
      <li class="page-item"><a class="page-link" href="#">3</a></li>
      <li class="page-item">
        <a class="page-link" href="#">Next</a>
      </li>
    </ul>
  </nav>


    <br><br><br><br><br>

    <!-- Option 1: Bootstrap Bundle with Popper -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>

    <!-- Option 2: Separate Popper and Bootstrap JS
    <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.3/dist/umd/popper.min.js" integrity="sha384-eMNCOe7tC1doHpGoWe/6oMVemdAVTMs2xqW4mwXrXsW0L84Iytr2wi5v2QjrP/xp" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.min.js" integrity="sha384-cn7l7gDp0eyniUwwAZgrzD06kc/tftFf19TOAs2zVinnD/C7E91j9yyk5//jjpt/" crossorigin="anonymous"></script> -->

</body>

</html>